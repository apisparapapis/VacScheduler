%ul.nav.nav-tabs
  %li=link_to tag_icon(:list, pat(:list)), url(:calendars, :index)
  %li.active=link_to tag_icon(:plus, pat(:new)), url(:calendars, :new)

.tabs-content
  -form_for :calendar, url(:calendars, :create), :class => 'form-horizontal' do |f|
    =partial 'calendars/form', :locals => { :f => f }
